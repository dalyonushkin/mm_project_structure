{
    "name": "mobile-monitoring-app",
    "author": "Borlas Group LLC",
    "homepage": "http://borlas.ru/",
    "private": true,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "npm run prepare:compilation&&ionic serve -b",
        "ionic:serve:win": "npm run prepare:compilation:win&&ionic serve -b",
        "test": "karma start ./test-config/karma.conf.js",
        "test-ci": "karma start ./test-config/karma.conf.js --single-run",
        "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
        "e2e": "npm run e2e-update && npm run e2e-test",
        "e2e-test": "protractor ./test-config/protractor.conf.js",
        "e2e-update": "webdriver-manager update --standalone false --gecko false",
        "sign": "build_sign.bat",
        "clean-apk": "if exist platforms\\android\\build\\outputs\\apk\\borlas.mobilemonitor.test.apk del platforms\\android\\build\\outputs\\apk\\borlas.mobilemonitor.test.apk && if exist platforms\\android\\build\\outputs\\apk\\borlas.mobilemonitor.apk del platforms\\android\\build\\outputs\\apk\\borlas.mobilemonitor.apk",
        "clean-apk2": "del platforms/android/build/outputs/apk/borlas.mobilemonitor.test.apk && del platforms/android/build/outputs/apk/borlas.mobilemonitor.apk",
        "build-debug": "npm run prepare:compilation && gradle -Pcustomer=$CUSTOMER_NAME updateCordovaAppVersion -Prelease.scope=patch && npm run clean-apk && ionic cordova build android --debug",
        "build-debug:win:phosagro": "npm run prepare:compilation:win && gradle -Pcustomer=phosagro updateCordovaAppVersion -Prelease.scope=patch && npm run clean-apk && ionic cordova build android --debug",
        "build-release": "npm run clean-apk && ionic cordova build android --release && npm run sign",
        "build-release:win:phosagro": "npm run prepare:compilation:win && gradle -Pcustomer=phosagro updateCordovaAppVersion -Prelease.scope=patch && npm run clean-apk && ionic cordova build android --release && npm run sign",
        "preprun": "ionic cordova prepare browser && ionic cordova run browser",
        "prepare:compilation": "npm run prepare:compilation:src&&npm run prepare:compilation:resources",
        "prepare:compilation:src": "rm -rdf src/compilations/current/*&&mkdir -p src/compilations/current&&cp -r src/compilations/base/* src/compilations/current/",
        "prepare:compilation:resources": "rm -rdf resources/*&&mkdir -p resources&&cp -r src/compilations/base/resources/* resources/",
        "prepare:compilation:win": "rd src\\compilations\\current /s /q&&md src\\compilations\\current&&xcopy src\\compilations\\base\\* src\\compilations\\current /e /q /y",
        "ionic:run:android": "npm run prepare:compilation&&gradle -Pcustomer=$CUSTOMER_NAME updateCordovaAppVersion -Prelease.scope=patch&&ionic cordova run android",
        "ionic:run:android:win:phosagro": "npm run prepare:compilation:win&&gradle -Pcustomer=phosagro updateCordovaAppVersion -Prelease.scope=patch&&ionic cordova run android",
        "ionic:run:android:win:eurocem": "npm run prepare:compilation:win&&gradle -Pcustomer=eurocem updateCordovaAppVersion -Prelease.scope=patch&&ionic cordova run android",
        "ionic:run:android:win:acron": "npm run prepare:compilation:win&&gradle -Pcustomer=acron updateCordovaAppVersion -Prelease.scope=patch&&ionic cordova run android",
        "ionic:run:android:win": "npm run prepare:compilation:win&&gradle -Pcustomer=%CUSTOMER_NAME% updateCordovaAppVersion -Prelease.scope=patch&&ionic cordova run android"
    },
    "config": {
        "ionic_copy": "./config/copy.config.js",
        "ionic_webpack": "./config/webpack.config.js"
    },
